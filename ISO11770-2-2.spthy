rule AB1:
  [ Fr(~kab1),!SharedKey($A, $B,kab)]
--[ Finish('success',~kab1) ]->
  [ Out(enc(~kab1,kab)) ]

 


rule Reveal:
    [ !SharedKey($A, $B,k)]
  --[ Reveal(k) ]->
    [ Out(k)]




